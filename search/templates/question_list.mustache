<div>
  {{#questions}}
    <h3><strong>{{{title}}}</strong></h3>
    <h4>{{time}}</h4>
    <div>{{{body}}}</div>
    <div class="col-md-12" style="padding-bottom: 15px; border-bottom:1px solid rgba(7,94,115,0.3);"></div>
  {{/questions}}
</div>

{{#questions}}
<article lang="es" class="post">
    <header>
      <span xmlns:dct="http://purl.org/dc/terms/" href="http://purl.org/dc/dcmitype/Text"
            property="dct:title" rel="dct:type" class="question">{{{title}}}</span>
      <a href="{{ url }}">{{ date }}</a>
    </header>
  <div class="answer">
    {{ body }}
  </div>
  {% include sharing.ext url=page.url title=page.title %}
</article>
